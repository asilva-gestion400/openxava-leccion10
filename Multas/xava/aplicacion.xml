<?xml version = "1.0" encoding = "ISO-8859-1"?><!DOCTYPE aplicacion SYSTEM "dtds/aplicacion.dtd"><aplicacion nombre="Multas"> 		<!--	Un módulo por defecto se asume para cada	controlador en <modulo-defecto/> 	-->	<modulo-defecto>		  		<controlador nombre="Typical"/>			</modulo-defecto>		<modulo nombre="MultasPagadas">		<modelo nombre="Multa"/>		<tab nombre="multasPagadas"/>		<controlador nombre="Typical"/>			<controlador nombre="MultasPagadas"/>	</modulo>		<modulo nombre="MultasPendientes">		<modelo nombre="Multa"/>		<tab nombre="multasPendientes"/>	</modulo>		<modulo nombre="VehiculoSimple">		<modelo nombre="Vehiculo"/>		<vista nombre="VehiculoSimple"/>		<controlador nombre="Typical"/>			<controlador nombre="VehiculoSimple"/>	</modulo>		<modulo nombre="CodigoPostal">		<modelo nombre="CodigoPostal"/>		<controlador nombre="TypicalNotResetOnSave"/>		<controlador nombre="CodigoPostal"/>	</modulo>		<modulo nombre="Multa">		<modelo nombre="Multa"/>		<controlador nombre="TypicalNotResetOnSave"/>		<controlador nombre="Multas"/>	</modulo>		<modulo nombre="MultaSoloDetalle">		<modelo nombre="Multa"/>		<controlador nombre="Typical"/>	    <controlador-modo nombre="Void"/> 	</modulo>		<modulo nombre="MultaSoloLista">		<var-entorno nombre="XAVA_LIST_ACTION" valor=""/>    		<modelo nombre="Multa"/>	    <controlador nombre="Typical"/>	    <controlador-modo nombre="ListOnly"/>  	</modulo>		<modulo nombre="MultaSoloLectura">		<var-entorno nombre="XAVA_SEARCH_ACTION" valor="CRUD.searchReadOnly"/>    	    <modelo nombre="Multa"/>	    <controlador nombre="Print"/>	</modulo>	</aplicacion>